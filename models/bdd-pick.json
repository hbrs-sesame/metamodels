{
    "@context": {
        "@base": "https://my.url/models/acceptance-criteria/",

        "bdd": "https://my.url/metamodels/acceptance-criteria/bdd#",
        "bdd-templ": "https://my.url/models/acceptance-criteria/bdd-templates/",
        "of-scenario":{ "@id": "bdd:of-scenario", "@type": "@id" },
        "has-var-connection": { "@id": "bdd:has-var-connection", "@type": "@id" },
        "of-variable": { "@id": "bdd:of-variable", "@type": "@id" },
        "has-variation": { "@id": "bdd:has-variation", "@type": "@id" },

        "hbrs-agents": "https://my.url/models/agents/brsu/",
        "hbrs-env": "https://my.url/models/environments/brsu/"
    },

    "@graph": [
        {
            "@id": "object-connection", "@type": "bdd:VariableConnection",
            "of-variable": "bdd-templ:pick-object",
            "has-variation": [ "hbrs-env:box", "hbrs-env:bottle" ]
        },
        {
            "@id": "workspace-connection", "@type": "bdd:VariableConnection",
            "of-variable": "bdd-templ:pick-workspace",
            "has-variation": [ "hbrs-env:dining-table" ]
        },
        {
            "@id": "robot-connection", "@type": "bdd:VariableConnection",
            "of-variable": "bdd-templ:pick-robot",
            "has-variation": [ "hbrs-agents:kinova1" ]
        },
        {
            "@id": "scenario-pick-1-arm", "@type": "bdd:ScenarioVariant",
            "of-scenario": "bdd-templ:scenario-pick",
            "has-var-connection": [
                "object-connection", "workspace-connection", "robot-connection"
            ]
        }
    ]
}